    {
      "name": "handleSubmit",
      "type": "method",
      "description": "",
      "body": "$ctrl.getSales()\n//   .then(data => {\n//     // Assuming $ctrl.fetchedData is where you store the fetched sales data\n//     $ctrl.fetchedSales = data;\n\n//     // Navigate to the 'results' state/page\n//     $ctrl.$state.go('results');\n//   })\n//   .catch(error => {\n//     // Handle error\n//     console.error('Error fetching sales:', error);\n//   });\n",
      "invokeOnLoad": false,
      "isAsync": false,
      "returnType": {
        "name": "any"
      },
      "parameters": []
    },
    {
      "name": "salesForecastingApi",
      "type": "open-api",
      "id": "65f8fb38e84fea8b10b8ee09"
    },
    {
      "name": "getSales",
      "type": "method",
      "description": "",
      "body": "if (!($ctrl.startDate instanceof Date && !isNaN($ctrl.startDate))) {\n    $ctrl.$notificationManager.show(\"Invalid date selected\", {\n        delay: 2500,\n        icon: 'error',\n        color: 'danger',\n    });\n} else {\n    // Get the year, month, and day\n    const year = $ctrl.startDate.getFullYear();\n    // Month starts from 0, so add 1 to get the correct month\n    const month = String($ctrl.startDate.getMonth() + 1).padStart(2, '0');\n    const day = String($ctrl.startDate.getDate()).padStart(2, '0');\n\n    // Construct the date string in YYYY-MM-DD format\n    const formattedDate = `${year}-${month}-${day}`;\n\n    // Then integrate this formattedDate into your function\n    $ctrl.salesForecastingApi.service_Main({\n        startDate: formattedDate, \n        forecastLen: $ctrl.forecastLen,\n        forecastRequestType: $ctrl.forecastRequestType,\n        productId: $ctrl.productIDinput || '8098965881074',\n        variantId: $ctrl.variantIDinput || '45184385286386',\n        ngrokSkipBrowserWarning: 'asd'\n    })\n    .then(response => {\n       $ctrl.fetchedData = response.data\n       $ctrl.$notificationManager.show(\"Successfully processed the request\", {\n          delay: 2500,\n          icon: 'check',\n          color: 'success',\n        });\n    })\n    .catch(error => {\n        // Handle error\n        console.error('Error fetching data:', error);\n        $ctrl.$notificationManager.show(\"No sales data found. Please try again.\", {\n          delay: 2500,\n          icon: 'error',\n          color: 'danger',\n        });\n    });\n}",
      "invokeOnLoad": false,
      "isAsync": false,
      "returnType": {
        "name": "any"
      },
      "parameters": []
    },
    {
      "name": "$notificationManager",
      "type": "service"
    }
